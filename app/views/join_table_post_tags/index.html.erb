<p id="notice"><%= notice %></p>

<h1>Listing Join Table Post Tags</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @join_table_post_tags.each do |join_table_post_tag| %>
      <tr>
        <td><%= link_to 'Show', join_table_post_tag %></td>
        <td><%= link_to 'Edit', edit_join_table_post_tag_path(join_table_post_tag) %></td>
        <td><%= link_to 'Destroy', join_table_post_tag, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

